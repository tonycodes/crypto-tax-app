{
  "name": "@crypto-tax-app/backend",
  "version": "1.0.0",
  "description": "Backend API server for crypto tax application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:test": "dotenv -e .env.test -- jest src/database/database.test.ts",
    "db:studio": "prisma studio"
  },
  "keywords": ["crypto", "tax", "api", "express", "prisma"],
  "author": "Tony",
  "license": "ISC",
  "dependencies": {
    "@crypto-tax-app/shared": "*",
    "@jup-ag/instruction-parser": "^6.0.28",
    "@mysten/sui": "^1.38.0",
    "@prisma/client": "^5.22.0",
    "@raydium-io/raydium-sdk-v2": "^0.2.26-alpha",
    "@solana/web3.js": "^1.98.4",
    "@types/uuid": "^10.0.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bip32": "^4.0.0",
    "bitcoinjs-lib": "^6.1.7",
    "bullmq": "^4.15.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "ecpair": "^2.1.0",
    "ethers": "^6.15.0",
    "express": "^4.18.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prisma": "^5.22.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
